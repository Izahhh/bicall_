{
  "expo": {
    "name": "Bicall",
    "slug": "Bicall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imgs/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/Fonts/Baloo.ttf"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/imgs/favicon.png"
    },
    "packagerOpts": {
      "assetExts": ["js", "json", "txt", "xml"],
      "sourceExts": ["expo.js", "expo.json", "js", "json", "ts", "tsx", "jsx"],
      "config": {
        "githubUrl": "https://github.com/y4smln/bicallMobile",
        "githubBranch": "master"
      }
    }
  }
}
